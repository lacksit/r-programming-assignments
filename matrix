A <- matrix(1:100,  nrow = 10)
B <- matrix(1:1000, nrow = 10)

dim(A)
dim(B) 

invA <- solve(A)  # cannot be done because it is singular det is 0
detA <- det(A)  # it returns 0


# fails because it is not a square

invB <- tryCatch(solve(B), error = function(e) e)
detB <- tryCatch(det(B),   error = function(e) e)
